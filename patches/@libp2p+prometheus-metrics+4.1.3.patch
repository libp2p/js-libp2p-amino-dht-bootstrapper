+        const totalMemoryMetric = this.registerMetric('nodejs_memory_total_bytes');
+        totalMemoryMetric.update(totalmem());
+        let previousUsage = process.cpuUsage();
+        let previousTime = Date.now();
+        const ncpu = cpus().length;
+        this.log('Collecting CPU metrics');
+        this.registerMetricGroup('nodejs_cpu_usage_percent', {
+            label: 'cpu',
+            calculate: () => {
+                const currentUsage = process.cpuUsage(previousUsage);
+                previousUsage = process.cpuUsage();
+                const currentTime = Date.now();
+                const timeDelta = (currentTime - previousTime) * ncpu;
+                const { user, system } = currentUsage;
+                previousTime = currentTime;
+                return {
+                    system: system / timeDelta,
+                    total: (system + user) / timeDelta,
+                    user: user / timeDelta
+                };
+            }
+        });
+        this.log('Collecting filesystem metrics');
+        this.registerMetricGroup('nodejs_fs_usage_bytes', {
+            label: 'filesystem',
+            calculate: async () => {
+                const stats = await statfs('/');
+                const total = stats.bsize * stats.blocks;
+                const available = stats.bsize * stats.bavail;
+                return {
+                    total,
+                    free: stats.bsize * stats.bfree,
+                    available,
+                    used: (available / total) * 100
+                };
+            }
+        });
