+        am.addObservedAddr(ma);
+        am.addObservedAddr(multiaddr(`${ma.toString()}/p2p/${peerId.toString()}`));
+        expect(am.getObservedAddrs()).to.have.lengthOf(1);
+        expect(am.getObservedAddrs().map(ma => ma.toString())).to.include(ma.toString());
