{
  "name": "@libp2p/amino-dht-bootstrapper",
  "version": "2.0.1",
  "description": "Run an Amino DHT bootstrapper with js-libp2p",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/libp2p/js-libp2p-amino-dht-bootstrapper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libp2p/js-libp2p-amino-dht-bootstrapper.git"
  },
  "bugs": {
    "url": "https://github.com/libp2p/js-libp2p-amino-dht-bootstrapper/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "bin": {
    "amino": "./bin/amino.js",
    "health-check": "./bin/amino-health-check.js"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "bin",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "breaking": true,
              "release": "major"
            },
            {
              "revert": true,
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "deps",
              "release": "patch"
            },
            {
              "scope": "no-release",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "chore",
                "section": "Trivial Changes"
              },
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "deps",
                "section": "Dependencies"
              },
              {
                "type": "test",
                "section": "Tests"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "build": "aegir build --bundle false",
    "test": "aegir test -t node",
    "test:node": "aegir test -t node --cov",
    "release": "aegir release",
    "dep-check": "aegir dep-check",
    "postinstall": "patch-package",
    "start": "npm run build && node ./dist/src/index.js"
  },
  "dependencies": {
    "@libp2p/autonat-v2": "^2.0.0",
    "@libp2p/bootstrap": "^12.0.0",
    "@libp2p/circuit-relay-v2": "^4.0.0",
    "@libp2p/crypto": "^5.1.5",
    "@libp2p/dcutr": "^3.0.0",
    "@libp2p/identify": "^4.0.0",
    "@libp2p/interface": "^3.0.0",
    "@libp2p/kad-dht": "^16.0.0",
    "@libp2p/keychain": "^6.0.0",
    "@libp2p/logger": "^6.0.0",
    "@libp2p/noise": "^1.0.0",
    "@libp2p/ping": "^3.0.0",
    "@libp2p/prometheus-metrics": "^5.0.0",
    "@libp2p/tcp": "^11.0.0",
    "@libp2p/tls": "^3.0.0",
    "@libp2p/utils": "^7.0.0",
    "@libp2p/webrtc": "^6.0.0",
    "@libp2p/websockets": "^10.0.0",
    "@libp2p/yamux": "^8.0.0",
    "@multiformats/multiaddr": "^13.0.1",
    "@multiformats/multiaddr-matcher": "^3.0.1",
    "datastore-level": "^12.0.2",
    "libp2p": "^3.0.0",
    "prom-client": "^15.1.3",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "aegir": "^47.0.14",
    "patch-package": "^8.0.0"
  }
}
